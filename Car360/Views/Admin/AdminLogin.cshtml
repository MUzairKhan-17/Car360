@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Car360</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="~/adminasset/img/logo.png" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/adminasset/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/adminasset/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/adminasset/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/adminasset/css/style.css" rel="stylesheet">
</head>

<body>
    @if (TempData["wrong"] != null)
    {
        <div class="alert alert-success"><strong>Success: @TempData["wrong"]</strong><button type="button" class="btn-close float-end" data-bs-dismiss="alert" aria-label="close"></button></div>
    }
    else if (TempData["Deactive"] != null)
    {
        <div class="alert alert-success"><strong>Success: @TempData["Deactive"]</strong><button type="button" class="btn-close float-end" data-bs-dismiss="alert" aria-label="close"></button></div>
    }
    else { }
<div class="container-xxl position-relative bg-light d-flex p-0">
    <!-- Login Start -->
    <div class="container-fluid">
        <div class="row h-100 align-items-center justify-content-center" style="min-height: 100vh;">
            <div class="col-12 col-sm-8 col-md-6 col-lg-5 col-xl-4">
                <div class="rounded p-4 my-4" id="login" style="min-width: 70vh;">
                    <div class="d-flex align-items-center justify-content-between mb-3">
                        <a>
                            <h3 id="logo-login"><i class="fa fa-hashtag me-2"></i>Car360</h3>
                        </a>
                        <h3 id="logo-login">Login</h3>
                    </div>
                    <form method="post">
                        <div class="form-floating mb-3">
                            <input type="email" class="form-control" id="floatingInput" placeholder="nameexample.com" name="txtmail" required>
                            <label for="floatingInput">Gamil:</label>
                        </div>
                        <div class="form-floating mb-4">
                            <input type="password" class="form-control" id="floatingPassword" placeholder="Password" name="txtpass" required>
                            <label for="floatingPassword">Password:</label>
                        </div>
                        <input type="submit" id="btn-login" class="btn py-3 w-100 mb-4" value="Login">
                    </form>
                    <span class="text-light text-center">@ViewBag.wrong</span>
                </div>
            </div>
        </div>
    </div>
    <!-- Login End -->
</div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/adminasset/lib/chart/chart.min.js"></script>
    <script src="~/adminasset/lib/easing/easing.min.js"></script>
    <script src="~/adminasset/lib/waypoints/waypoints.min.js"></script>
    <script src="~/adminasset/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/adminasset/js/main.js"></script>
</body>

</html>