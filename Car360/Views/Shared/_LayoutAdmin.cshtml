<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Car360</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="~/adminasset/img/logo.png" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" rel="stylesheet" />

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/adminasset/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/adminasset/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/adminasset/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/adminasset/css/style.css" rel="stylesheet">
</head>

<body>
    <div class="container-fluid position-relative bg-white d-flex p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->
        <!-- Sidebar Start -->
        <div class="sidebar pe-4 pb-3">
            <nav class="navbar bg-light navbar-success">
                <a asp-action="Index" asp-controller="Admin" class="navbar-brand mx-4 mb-3">
                    <h3 id="logo-index"><i class="fa fa-hashtag me-2"></i>Car360</h3>
                </a>
                <div class="navbar-nav w-100">
                    <a asp-action="Index" asp-controller="Admin" class="nav-item nav-link"><i class="fa fa-tachometer-alt me-2"></i>Dashboard</a>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown"><i class="fa fa-users me-2"></i>User's</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a asp-action="UserActiveDeactive" asp-controller="Admin" class="nav-link">Active/Deactive User's</a>
                            <a asp-action="UserDelete" asp-controller="Admin" class="nav-link">Delete User's</a >
                        </div>
                    </div>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown"><i class="fa fa-car me-2"></i>Product</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a asp-action="ProductInsert" asp-controller="Admin" class="nav-link">Insert Product's</a>
                            <a asp-action="ProductFetch" asp-controller="Admin" class="nav-link">Fetch Product's</a>
                            <a asp-action="ProductDelete" asp-controller="Admin" class="nav-link">Delete Product's</a>
                            <a asp-action="ProductUpdate" asp-controller="Admin" class="nav-link">Update Product's</a>
                            <a asp-action="ProductActiveDeactive" asp-controller="Admin" class="nav-link">Active/Deactive Product's</a>
                        </div>
                    </div>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown"><i class="fa fa-user-tie me-2"></i>Admin</a>
                        <div class="dropdown-menu bg-transparent border-0">
                            <a asp-action="AdminSignup" asp-controller="Admin" class="nav-link">Add Admin</a>
                            <a asp-action="AdminFetch" asp-controller="Admin" class="nav-link">Fetch Admin</a>
                            <a asp-action="AdminDelete" asp-controller="Admin" class="nav-link">Delete Admin</a>
                            <a asp-action="AdminActiveDeactive" asp-controller="Admin" class="nav-link">Active/Deactive Admin</a>
                        </div>
                    </div>
                    <a asp-action="Appointment" asp-controller="Admin" class="nav-link">Fetch Appointment</a>
                    <a asp-action="DeleteAppointment" asp-controller="Admin" class="nav-link">Delete Appointment</a>
                </div>
            </nav>
        </div>
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            <nav class="navbar navbar-expand sticky-top px-4 py-0" id="navbar-index">
                <a href="#" class="sidebar-toggler flex-shrink-0">
                    <i class="fa fa-bars" id="toggle"></i>
                </a>
                <div class="navbar-nav align-items-center ms-auto">
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown" id="profile-hover">
                            <span class="d-none d-lg-inline-flex">Options</span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end bg-light border-0 rounded-0 rounded-bottom m-0" id="dropdown-profile">
                            <a asp-action="AdminProfile" asp-controller="Admin" class="dropdown-item" id="drop-profile">My Profile</a>
                            <a asp-action="AdminLogout" asp-controller="Admin" class="dropdown-item" id="drop-profile">Log Out</a>
                        </div>
                    </div>
                </div>
            </nav>
            <!-- Navbar End -->

                        @* renderbody start *@

                        <div>
                            @RenderBody()
                        </div>
                        
                        @* renderbody end *@

            <!-- Footer Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="bg-dark rounded-top p-4">
                    <div class="row">
                        <div class="col-12 col-sm-6 text-center text-light text-sm-start">
                            &copy; <a href="#" id="toggle">Website-ASP</a>, All Right Reserved.
                        </div>
                        <div class="col-12 col-sm-6 text-center text-light text-sm-end">
                            Designed By UK Code's
                            </br>
                            Distributed By <a class="border-bottom" id="toggle" target="_blank">UK Theme's</a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Footer End -->
        </div>
        <!-- Content End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-lg-square back-to-top" id="btn-arrow"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/adminasset/lib/chart/chart.min.js"></script>
    <script src="~/adminasset/lib/easing/easing.min.js"></script>
    <script src="~/adminasset/lib/waypoints/waypoints.min.js"></script>
    <script src="~/adminasset/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/adminasset/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/adminasset/js/main.js"></script>
    <script src="~/adminasset/js/script.js"></script>
</body>

</html>